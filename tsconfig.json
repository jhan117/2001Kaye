{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@public/*": [
        "public/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@styles-components/*": [
        "src/styles/components/*"
      ],
      "@styles-layouts/*": [
        "src/styles/layouts/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ]
    }
  }
}